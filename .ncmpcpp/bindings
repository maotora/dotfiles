def_key "k"
  scroll_up

def_key "j"
  scroll_down

def_key "h"
  previous_column

def_key "l"
  next_column

def_key "ctrl-f"
  page_down

def_key "ctrl-b"
  page_up

def_key "p"
  pause

def_key "K"
  move_selected_items_up

def_key "J"
  move_selected_items_down

def_key "n"
  next_found_item

def_key "N"
  previous_found_item

def_key "g"
  move_home

def_key "G"
  move_end

def_key "ctrl-l"
  show_lyrics

def_key "h"
  jump_to_parent_directory
